{
  "name": "practice-app",
  "version": "1.0.0",
  "description": "An app for practicing implementing the web application",
  "main": "index.js",
  "repository": "https://github.com/maaaashin324/practice-app.git",
  "author": "maaaashin324 <masataka.shintoku@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "test": "jest",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "react-scripts build",
    "front-test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "migrate": "./node_modules/knex/bin/cli.js migrate:latest --knexfile ./database/knexfile.js",
    "rollback": "./node_modules/knex/bin/cli.js migrate:rollback --knexfile ./database/knexfile.js",
    "startdb": "docker start mydb",
    "stopdb": "docker stop mydb"
  },
  "dependencies": {
    "body-parser": "~1.18.3",
    "concurrently": "^4.1.0",
    "express": "~4.16.4",
    "humps": "2.0.1",
    "knex": "~0.16.3",
    "pg": "~7.8.0",
    "react": "~16.7.0",
    "react-dom": "~16.7.0",
    "react-scripts": "2.1.3"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "jest": "23.6.0",
    "supertest": "3.4.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
